    "c8": "^10.1.3",
    "eslint": "^9.35.0",
    "eslint-plugin-vue": "^10.4.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "vite": "^7.1.4",
    "vite-plugin-eslint": "^1.8.1",
