{
  "name": "portafolio",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=24.7.0",
    "npm": ">=11.0.0"
  },
  "scripts": {
    "dev": "vite",
    "serve": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint --config eslint.config.mjs src/**/*.js",
    "lint:fix": "eslint --config eslint.config.mjs src/**/*.js --fix",
    "lint:check": "eslint --config eslint.config.mjs src/**/*.js --max-warnings 0",
    "seo:optimize": "node scripts/seo-optimizer.js",
    "build:seo": "npm run seo:optimize && npm run build"
  },
  "dependencies": {
    "@babel/core": "^7.29.0",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/vue-fontawesome": "^3.1.3",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.8",
    "core-js": "^3.48.0",
    "jquery": "^4.0.0",
    "lodash": "^4.17.23",
    "popper.js": "^1.16.1",
    "vue": "^3.5.27",
    "vue-eslint-parser": "^10.2.0",
    "vue-i18n": "^11.2.8"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.6",
    "@babel/preset-env": "^7.29.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/compiler-sfc": "^3.5.27",
    "c8": "^10.1.3",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^10.7.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^28.0.0",
    "terser": "^5.46.0",
    "vite": "^7.3.1",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^4.0.18"
  },
  "browserslist": [
    "Chrome >= 90",
    "Firefox >= 88",
    "Safari >= 14",
    "Edge >= 90",
    "not IE 11",
    "not dead",
    "not op_mini all"
  ]
}
